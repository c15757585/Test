//My initial idea was to have the player quickly create a map
//for a friend to play but was quite overwhelmed with the amount
//of information I had to learn so I broke it down into this
//My last hurdle was to make the level jump when enough points were earned.


int score;
int lives;
boolean textShake = false;
int shakeTimer = 50;
Paddle paddle = new Paddle();
Ball ball = new Ball();
int col = 51;
int row = 42;
Brick[][] block = new Brick[row][col];

float paddleX, paddleY;
int paddleWidth = 75;
float theta;
boolean stop;

import ddf.minim.*;
Minim minim;
AudioPlayer song, hit;


void setup() {
  size(500, 600);
  noStroke();
  smooth();
  paddle.reset();
  ball.reset();
 
  minim = new Minim(this);  
  song = minim.loadFile("Patterns.mp3");
  hit = minim.loadFile("Punch.mp3");
  song.play();
  song.loop();
 
  lives = 3;
  score = 0;
  //set up the board based off of 1, 0 image
  //change the line lvl1 to lvl2 to see another level and so on
  for (int i = 0; i < lvl1.length; i++) {
    for (int j = 0; j < 50; j++) {
      block[i][j] = new Brick(i*12, j*12, lvl1[i][j]);
    }
  }
}

void draw() {
  background(0);

  //change to the level you have selected here
  for (int i = 0; i < lvl1.length; i++) {
    for (int j = 0; j < 50; j++) {
      block[i][j].display();
    }
  }
  paddle.display(mouseX);
  ball.display();
  
  //If ball y position lower then paddle by 10 use reset 
    if (ball.yPos() > paddleY + 10) {
      ball.reset();
      lives = lives -1;
    
    }
    if (textShake == true) {
      translate(random(10), random(10));
      shakeTimer--;
  }
  
  //mapping bricks and collisions
  int hitx = int(map(ball.xPos(), 0, 500, 0, 41));
  int hity = int(map(ball.yPos(), 0, 600, 0, 50));
  
  if (hitx < 1) {
    hitx = 2;
  }
  if (hity < 1) {
    hity = 2;
  }
  

  if (ball.movingUp()) {
    //ball can either hit up, down, left or right
    //check hitbox above
    if (ball.up() < block[hitx][hity-1].down() ) {
      if (block[hitx][hity-1].isWhite()) {
        block[hitx][hity-1].changeColor();
        ball.changeY();
        score = score+10;
        textShake = true;
        hit.play();
        hit.rewind();
        //This will reset the sound effect Punch so it plays again
        //however it will not reset it quick enough to play out
        //all the collisions, which to be honest was a relief
        //to find out.
      }
    }
    
  }
  //if the ball is moving down
  if (!ball.movingUp()) {

    //check down
   if (ball.down() < block[hitx][hity+1].down() ) {
      if (block[hitx][hity+1].isWhite()) {
        block[hitx][hity+1].changeColor();
        ball.changeY();
        score = score+10;
        textShake = true;
        hit.play();
        hit.rewind();
      }
    }
  }
 
   if (ball.left() > block[hitx+1][hity].right() ) {
      if(block[hitx+1][hity].isWhite()) {
        block[hitx+1][hity].changeColor();
        ball.changeX();
        score = score+10;
        textShake = true;
        hit.play();
        hit.rewind();
      }
    }
  
   if (ball.right() < block[hitx-1][hity].left()) {
      if(block[hitx-1][hity].isWhite()) {
        block[hitx-1][hity].changeColor();
        ball.changeX();
        score = score+10;
        textShake = true;
        hit.play();
        hit.rewind();
      }
    }
   if (shakeTimer==0) {
      textShake = false;
      shakeTimer = 50;
  }
    //display score and lives
    
    textSize(20);
    textAlign(LEFT);
    text("Score: " + score, 20, 20);
    
    textSize(20);
    textAlign(CENTER);
    text("Lives: " + lives, 450, 20);

    //reset game 
  if(lives == 0)  {
    ball.reset();
    text("GAMER OVER: " + score, width/2, height/2);
    if(mousePressed){
      lives = 3;
      score = 0;
      song.pause();
      setup();
      }
    }
}

